<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id="mybean" class="com.javalearning.ajay.TestingObject">
    	<property name="objs">
    		<list>
    			<ref bean="value1"/>
    			<ref bean="value2"/>
    			<ref bean="value3"/>
    		</list>
    	
    	</property>
    </bean>
    
    <bean id="value1" class="com.javalearning.ajay.SmallObjects">
    	<property name="value" value="10"></property>
    </bean>
    
    <bean id="value2" class="com.javalearning.ajay.SmallObjects">
    	<property name="value" value="20"></property>
    </bean>
    
    <bean id="value3" class="com.javalearning.ajay.SmallObjects">
    	<property name="value" value="30"></property>
    </bean>
</beans>